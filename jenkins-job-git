pipeline{
    agent any
    
    environment{
         path= "/usr/local/apache-maven"

    }
    stages{
        stage(" git check out"){
            steps{
                git branch: 'main', credentialsId: 'purushothamaPp', url: 'https://github.com/purushothamaPp/jenkinscicd/'
            }
        }
        stage("maven build){
            steps{
                sh "mvn clean package"                  
            }
        }
    }
}
    
